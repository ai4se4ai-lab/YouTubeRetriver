# Server configuration
PORT=3000
NODE_ENV=development

# Agent approval settings
# Format: Comma-separated list of agent names that require approval
# Example: contentAnalysis,knowledgeRetrieval,analogyGeneration
# Use "all" to require approval for all agents
# Use "none" or leave empty to skip all approvals
REQUIRED_AGENT_APPROVALS=all

# Google OAuth credentials
# Create these at https://console.developers.google.com/
GOOGLE_CLIENT_ID=TEST
GOOGLE_CLIENT_SECRET=TEST
GOOGLE_REDIRECT_URI=http://localhost:3000/api/auth/callback

# YouTube API key (optional, but recommended for better rate limits)
# YOUTUBE_API_KEY=TEST

# Security settings
SESSION_SECRET=TEST!
CORS_ORIGIN=http://localhost:3000

# Storage location for temporary files
TEMP_DIR=./temp

# OpenAI API key for LLM agents
OPENAI_API_KEY=TEST

# Note: Git repository settings have been moved to gitConfig.json